<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vigor.vigor">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:name="com.example.vigor.vigor.VolleySingleton"

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.vigor.vigor.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.vigor.vigor.TestActivity" />
        <activity android:name="com.example.vigor.vigor.Graphing"></activity>
        <activity android:name="com.example.vigor.vigor.StepActivity"></activity>
    </application>

</manifest>